{
        "root": true,
        "env": {
                "browser": true,
                "es2021": true,
                "node": true
        },
        "parser": "vue-eslint-parser",
        "parserOptions": {
                "parser": "@typescript-eslint/parser",
                "ecmaVersion": 2020,
                "sourceType": "module"
        },
        "extends": [
                "@nuxtjs/eslint-config-typescript",
                "plugin:vue/vue3-recommended",
                "plugin:@typescript-eslint/recommended",
                "prettier"
        ],
        "rules": {
                "vue/multi-word-component-names": "off"
        }
}
